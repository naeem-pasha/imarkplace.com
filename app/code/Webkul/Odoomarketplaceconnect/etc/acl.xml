<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Webkul_Odoomagentoconnect::manager" title="Odoo Connector" sortOrder="10" >                    
                    <resource id="Webkul_Odoomarketplaceconnect::marketplace" title="Marketplace" sortOrder="10" >
                        <resource id="Webkul_Odoomarketplaceconnect::sellers" title="Sellers Mapping" sortOrder="10" >
                            <resource id="Webkul_Odoomarketplaceconnect::sellers_list" title="List View" sortOrder="10"/>
                            <resource id="Webkul_Odoomarketplaceconnect::sellers_new" title="Create" sortOrder="20"/>
                            <resource id="Webkul_Odoomarketplaceconnect::sellers_view" title="View" sortOrder="30"/>
                            <resource id="Webkul_Odoomarketplaceconnect::sellers_save" title="Save" sortOrder="40"/>
                        </resource>
                        <resource id="Webkul_Odoomarketplaceconnect::category" title="Category Mapping" sortOrder="10" >
                            <resource id="Webkul_Odoomarketplaceconnect::category_list" title="List View" sortOrder="10"/>
                            <resource id="Webkul_Odoomarketplaceconnect::category_new" title="Create" sortOrder="20"/>
                            <resource id="Webkul_Odoomarketplaceconnect::category_view" title="View" sortOrder="30"/>
                            <resource id="Webkul_Odoomarketplaceconnect::category_save" title="Save" sortOrder="40"/>
                        </resource>
                        <resource id="Webkul_Odoomarketplaceconnect::product" title="Products Mapping" sortOrder="10" >
                            <resource id="Webkul_Odoomarketplaceconnect::product_list" title="List View" sortOrder="10"/>
                            <resource id="Webkul_Odoomarketplaceconnect::product_new" title="Create" sortOrder="20"/>
                            <resource id="Webkul_Odoomarketplaceconnect::product_view" title="View" sortOrder="30"/>
                            <resource id="Webkul_Odoomarketplaceconnect::product_save" title="Save" sortOrder="40"/>
                        </resource>
                        <resource id="Webkul_Odoomarketplaceconnect::order" title="Orders Mapping" sortOrder="10" >
                            <resource id="Webkul_Odoomarketplaceconnect::order_list" title="List View" sortOrder="10"/>
                            <resource id="Webkul_Odoomarketplaceconnect::order_new" title="Create" sortOrder="20"/>
                            <resource id="Webkul_Odoomarketplaceconnect::order_view" title="View" sortOrder="30"/>
                            <resource id="Webkul_Odoomarketplaceconnect::order_save" title="Save" sortOrder="40"/>
                        </resource>
                        <resource id="Webkul_Odoomarketplaceconnect::transaction" title="Seller Transactions" sortOrder="10" >
                            <resource id="Webkul_Odoomarketplaceconnect::transaction_list" title="List View" sortOrder="10"/>
                            <resource id="Webkul_Odoomarketplaceconnect::transaction_new" title="Create" sortOrder="20"/>
                            <resource id="Webkul_Odoomarketplaceconnect::transaction_view" title="View" sortOrder="30"/>
                            <resource id="Webkul_Odoomarketplaceconnect::transaction_save" title="Save" sortOrder="40"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
