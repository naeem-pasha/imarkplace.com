    <event name="odoomagentoconnect_mapping_delete_after">
        <observer name="webkul_odoomarketplaceconnect_mapping_delete_after_observer" instance="Webkul\Odoomarketplaceconnect\Observer\MappingDeleteAfterObserver" />
    </event>
</config>
